{$CADDY_API_WEB_ADDRESS} {
	cors
	root /srv
	gzip
	proxy /api/ server:10000 {
	      	   without /api/
		   transparent
	}
	proxy /docs/server/ server_docs:6060/pkg/ {
		without /docs/server/
		#transparent
	}
	proxy /docs/josh/ josh_docs:6061/pkg/ {
		without /docs/josh/
		#transparent
	}
	log / /var/log/access.log "{combined}"
	errors stdout
}
